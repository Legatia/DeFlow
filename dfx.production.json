{
  "canisters": {
    "DeFlow_backend": {
      "candid": "src/DeFlow_backend/DeFlow_backend.did",
      "package": "DeFlow_backend",
      "type": "rust"
    },
    "DeFlow_pool": {
      "candid": "src/DeFlow_pool/DeFlow_pool.did",
      "package": "deflow_pool",
      "type": "rust"
    },
    "DeFlow_frontend": {
      "dependencies": [
        "DeFlow_backend",
        "DeFlow_pool"
      ],
      "source": [
        "src/DeFlow_frontend/dist"
      ],
      "type": "assets"
    },
    "DeFlow_admin": {
      "dependencies": [
        "DeFlow_pool"
      ],
      "source": [
        "src/DeFlow_admin/dist"
      ],
      "type": "assets"
    }
  },
  "networks": {
    "ic": {
      "providers": [
        "https://ic0.app"
      ],
      "type": "persistent"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "output_env_file": ".env",
  "version": 1
}